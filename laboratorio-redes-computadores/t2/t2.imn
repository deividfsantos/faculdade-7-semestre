node n1 {
    type router
    model host
    network-config {
	hostname Atacante
	!
	interface eth0
	 ip address 10.0.0.10/24
	!
    }
    canvas c1
    iconcoords {88.0 287.0}
    labelcoords {88.0 319.0}
    interface-peer {eth0 n5}
}

